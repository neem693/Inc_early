<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
    namespace="com.anylogic.iot.api.admin.onm.mapper.AuthmenuMapper">


    <!--  권한-메뉴 - insertAuthMenu -->
    <insert id="insertAuthMenu" parameterType="Map">
        INSERT INTO
        sdp.tb_authmenu(
            auth_id
            , menu_id
            , del_yn
        )
        VALUES(
            #{auth_id}::int
            , #{menu_id}
            , 'N' 
        )
    </insert>

</mapper>
